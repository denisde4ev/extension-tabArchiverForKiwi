{
	"manifest_version": 3,
	"name": "Tab Archiver",
	"description": "Extension targeting Android Chromium (Kiwi) to archive tabs.",
	"version": "1.0",
	"permissions": [
		"tabs",
		"storage"
	],
	"action": {
		"default_title": "Archive Tabs"
	},
	"options_page": "options.html",
	"background": {
		"service_worker": "background.js"
	}
}
